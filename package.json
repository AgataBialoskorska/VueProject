{
    "name": "portfolio",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview --port 5173",
        "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs --fix --ignore-path .gitignore",
        "format": "prettier --write src/",
        "cypress": "cypress open",
        "e2e-open": "start-server-and-test dev http://localhost:5173 \"cypress open --e2e\"",
        "e2e-run": "start-server-and-test dev http://localhost:5173 \"cypress run --e2e\"",
        "e2e-prod": "vite build && start-server-and-test preview http://localhost:5173 \"cypress run --e2e\""
    },
    "dependencies": {
        "axios": "1.7.7",
        "dotenv": "16.4.5",
        "follow-redirects": ">=1.15.9",
        "oh-vue-icons": "1.0.0-rc3",
        "vue": "3.5.4",
        "start-server-and-test": "2.0.7",
        "vue-material-design-icons": "5.3.0",
        "vue-router": "4.4.3"
    },
    "devDependencies": {
        "@rushstack/eslint-patch": "1.10.3",
        "@vitejs/plugin-vue": "5.0.5",
        "@vue/eslint-config-prettier": "9.0.0",
        "cypress": "13.12.0",
        "eslint": "9.6.0",
        "eslint-plugin-cypress": "3.3.0",
        "eslint-plugin-vue": "9.26.0",
        "prettier": "3.3.2",
        "vite": "5.4.8",
        "follow-redirects": ">=1.15.9"
    }
}
